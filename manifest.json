{
    "name": "YoutubeAds",
    "version": "0.1",
    "description": "Skip ads on youtube",
    "permissions": [
        "activeTab"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_title": "Skip Ad"
    },
    "content_scripts": [
        {
            "matches": [
                "https://www.youtube.com/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "manifest_version": 3
}